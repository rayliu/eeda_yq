		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	oe.logo_path,
	oe.office_name,
	pown.logo_path p_logo_path,
	pown.company_name p_company_name,
	jos.order_id,
	josi.order_id,
	jos.mbl_no,
	jos.hbl_no,
	josi.container_type,
	josi.container_no,
	CONCAT(lo. NAME,'-',lo.code) pol_name,
	CONCAT(lo1. NAME,'-',lo1.code) fnd_name,
 	CONCAT(jos.vessel,'/',jos.voyage) vessel_voyage,
	IFNULL(p.company_name_eng,p.company_name) HBLshipperName,
	IFNULL(p1.company_name_eng,p1.company_name) HBLconsigneeName
FROM
	job_order_shipment jos
LEFT JOIN job_order_shipment_item josi ON jos.order_id = josi.order_id
LEFT JOIN location lo ON lo.id = jos.pol
LEFT JOIN location lo1 ON lo1.id = jos.fnd
LEFT JOIN party p ON p.id = jos.HBLshipper
LEFT JOIN party p1 ON p1.id = jos.HBLconsignee
LEFT JOIN user_login ul on ul.id = $P{login_user_id}
LEFT JOIN office oe ON oe.id = ul.office_id
LEFT JOIN (SELECT * from party where type='OWN') pown on  pown.office_id = ul.office_id
where jos.order_id = $P{order_id}]]>
	</queryString>
