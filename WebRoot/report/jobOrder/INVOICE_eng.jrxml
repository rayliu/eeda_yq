	<queryString>
		<![CDATA[SELECT
	(SELECT   CONCAT('A/C Name:',GROUP_CONCAT( DISTINCT bank_name)) from fin_account WHERE id in (jo.account_ids)) bankName,
	(SELECT   CONCAT('Bank Name:',GROUP_CONCAT( DISTINCT account_name)) from fin_account WHERE id in (jo.account_ids)) accountName,
	(SELECT GROUP_CONCAT(accountNo SEPARATOR '\n')
	from
	(SELECT CONCAT(fa.currency,' Number:',fa.account_no) accountNo from fin_account fa
	WHERE $X{IN,fa.id,account_ids}) A) accountNo,
	(SELECT GROUP_CONCAT(accountNo SEPARATOR '\n')
	from
	(SELECT CONCAT(fa.currency,' Number:',fa.account_no) accountNo from fin_account fa
	WHERE $X{IN,fa.id,account_ids}) A) accountNo,
	jor.sp_id,jor.exchange_total_amount exchange_total_amount,jor.invoice_no invoice_no,jor.price price,jor.total_amount total_amount,
  jor.exchange_currency_rate exchange_rate,jor.amount amount, ifnull(pr.company_name_eng,pr.company_name) companyName,
ifnull(pr.contact_person_eng,pr.contact_person) personEng,
	ifnull(f.name_eng,f.name) charge_name,
	u. NAME unit_name,
	c. NAME currency_name,
	IFNULL(jo.gross_weight,josi.gross_weight) gross_weight,
	IFNULL(jo.pieces,josi.pieces) pieces,

	josi.container_no,
	jos.order_id,
	jos.hbl_no,
	jos.mbl_no,
	jos.etd,
	jos.eta,
	jo.ref_no,
	jo.order_no,
  	jo.order_export_date,
	Upper(lo. NAME) fnd,
	Upper(lo1. NAME) pol,
	u1.name_eng ocean_unit,
  u. NAME unit_name,
	IFNULL(
		pr.company_name_eng,
		pr.company_name
	) company_name,
	IFNULL(pr.address_eng, pr.address) address,
	pr.phone,
	(select GROUP_CONCAT(total SEPARATOR '\n') from (SELECT
			CONCAT(c.name,': ',convert(sum(total_amount),decimal(10,2))) total
		FROM
			job_order_arap jor
		LEFT JOIN currency c ON c.id = jor.currency_id
		WHERE
			jor.order_type = 'charge'
AND $X{IN,jor.id,order_id}
GROUP BY c.id) a
	) total,CONCAT(jos.vessel,'/',jos.voyage) vessel_voyage,

(select GROUP_CONCAT(total SEPARATOR '\n') from (SELECT
			CONCAT(c.name,': ',convert(sum(exchange_total_amount),decimal(10,2))) total
		FROM
			job_order_arap joa
		LEFT JOIN currency c ON c.id = joa.exchange_currency_id
		WHERE
			joa.order_type = 'charge'
			AND $X{IN,joa.id,order_id} GROUP BY c.id) a
	) ex_total
FROM
	 job_order jo
LEFT JOIN job_order_arap jor ON jo.id = jor.order_id
LEFT JOIN party pr ON pr.id = jor.sp_id
LEFT JOIN job_order_shipment jos ON jos.order_id = jor.order_id
LEFT JOIN job_order_shipment_item josi ON josi.order_id = jo.id
LEFT JOIN unit u1 ON josi.unit_id = u1.id
LEFT JOIN unit u ON u.id = jor.unit_id
LEFT JOIN fin_item f ON f.id = jor.charge_id
LEFT JOIN currency c ON c.id = jor.currency_id
LEFT JOIN location lo1 ON lo1.id = jos.pol
LEFT JOIN location lo ON lo.id = jos.fnd
WHERE
	jor.order_type = 'charge'
AND  $X{IN,jor.id,order_id} group by jor.id]]>
	</queryString>
