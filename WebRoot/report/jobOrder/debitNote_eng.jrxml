	<queryString>
		<![CDATA[SELECT
(SELECT   CONCAT('A/C Name:',GROUP_CONCAT( DISTINCT bank_name)) from fin_account WHERE id in (jo.account_ids)) bankName,
	(SELECT   CONCAT('Bank Name:',GROUP_CONCAT( DISTINCT account_name)) from fin_account WHERE id in (jo.account_ids)) accountName,
	(SELECT GROUP_CONCAT(accountNo SEPARATOR '\n')
	from
	(SELECT CONCAT(fa.currency,' Number:',fa.account_no) accountNo from fin_account fa
	WHERE $X{IN,fa.id,account_ids}) A) accountNo,
	joa.sp_id,joa.exchange_total_amount exchange_total_amount,joa.invoice_no invoice_no,joa.price price,joa.total_amount total_amount,
  joa.exchange_currency_rate exchange_rate,joa.amount amount, pr.company_name_eng companyName,IFNULL(pr.contact_person_eng,pr.contact_person) personEng,
	ifnull(f.name_eng,f.name) charge_name_eng,
	u. NAME unit_name,
	c. NAME currency_name,
	IFNULL(jo.gross_weight,josi.gross_weight) gross_weight,
	CONCAT(CAST(IFNULL(jo.pieces,josi.pieces) AS char),' ','dfas') pieces,
	o.logo_path,
	o.office_name,
	o.eng_office_name,
	o.address,
	o.eng_address,
	CONCAT('Fax: ',o.office_fax) office_fax,
	CONCAT('Tel: ',o.office_tel) office_tel,
	pown.logo_path p_logo_path,
	pown.company_name p_company_name,
	pown.company_name_eng p_company_name_eng,
	pown.address p_address,
	pown.address_eng p_address_eng,
	CONCAT('Fax: ',pown.fax) p_fax,
	CONCAT('Tel: ',pown.phone) p_phone,
	josi.container_no,
	jos.order_id,
	jos.hbl_no,
	jos.mbl_no,
	jos.etd,
	jos.eta,
	jo.ref_no,
	jo.order_no,
	jo.order_export_date order_export_date,
	Upper(lo. NAME) fnd,
	Upper(lo1. NAME) pol,
	u1.name_eng ocean_unit,
  u. NAME unit_name,
	IFNULL(
		pr.company_name_eng,
		pr.company_name
	) company_name,
	IFNULL(pr.address_eng, pr.address) address,
	pr.phone,
	(select GROUP_CONCAT(total SEPARATOR '\n') from (SELECT
			CONCAT(c.name,': ',sum(total_amount)) total
		FROM
			job_order_arap joa
		LEFT JOIN currency c ON c.id = joa.currency_id
		WHERE
			joa.order_type = 'charge'
AND $X{IN,joa.id,order_id}
GROUP BY c.id) a
	) total,CONCAT(jos.vessel,'/',jos.voyage) vessel_voyage,

(select GROUP_CONCAT(total SEPARATOR '\n') from (SELECT
			CONCAT(c.name,': ',convert(sum(exchange_total_amount),decimal(10,2))) total
		FROM
			job_order_arap joa
		LEFT JOIN currency c ON c.id = joa.exchange_currency_id
		WHERE
			joa.order_type = 'charge'
			AND $X{IN,joa.id,order_id} GROUP BY c.id) a
	) ex_total
FROM
	 job_order jo
LEFT JOIN job_order_arap joa ON jo.id = joa.order_id
LEFT JOIN party pr ON pr.id = joa.sp_id
LEFT JOIN office o on o.id = pr.office_id
LEFT JOIN (SELECT * from party where type='OWN') pown on  pown.office_id = o.id
LEFT JOIN job_order_shipment jos ON jos.order_id = joa.order_id
LEFT JOIN job_order_shipment_item josi ON josi.order_id = jo.id
LEFT JOIN unit u1 ON josi.unit_id = u1.id
LEFT JOIN unit u ON u.id = joa.unit_id
LEFT JOIN fin_item f ON f.id = joa.charge_id
LEFT JOIN currency c ON c.id = joa.currency_id
LEFT JOIN location lo1 ON lo1.id = jos.pol
LEFT JOIN location lo ON lo.id = jos.fnd
WHERE
	joa.order_type = 'charge'
AND   $X{IN,joa.id,order_id} group by joa.id]]>
	</queryString>
