	<queryString>
		<![CDATA[SELECT
	jor.sp_id,jor.currency_total_amount currency_total_amount,jor.invoice_no invoice_no,jor.price price,jor.total_amount total_amount,
  jor.exchange_rate exchange_rate,jor.amount amount, pr.company_name_eng companyName,pr.contact_person_eng personEng,
	f.name_eng charge_name_eng,
	u. NAME unit_name,
	c. NAME currency_name,
	josi.gross_weight,
	josi.pieces,

	josi.container_no,
	jos.order_id,
	jos.hbl_no,
	jos.mbl_no,
	jos.etd,
	jos.eta,
	jo.ref_no,
	jo.order_no,
	Upper(lo. NAME) fnd,
	Upper(lo1. NAME) pol,
	u1.name_eng ocean_unit,
  u. NAME unit_name,
	IFNULL(
		pr.company_name_eng,
		pr.company_name
	) company_name,
	IFNULL(pr.address_eng, pr.address) address,
	pr.phone,
	(
		SELECT
			sum(total_amount)
		FROM
			job_order_arap jor
		LEFT JOIN currency c ON c.id = jor.currency_id
		WHERE
			jor.order_type = 'charge'
		AND c. NAME = 'CNY'
		AND $X{IN,jor.id,order_id}
	) total_RMB,
	(
		SELECT
			sum(total_amount)
		FROM
			job_order_arap jor
		LEFT JOIN currency c ON c.id = jor.currency_id
		WHERE
			jor.order_type = 'charge'
		AND c. NAME = 'USD'
		AND $X{IN,jor.id,order_id}
	) total_USD,
	(
		SELECT
			sum(total_amount)
		FROM
			job_order_arap jor
		LEFT JOIN currency c ON c.id = jor.currency_id
		WHERE
			jor.order_type = 'charge'
		AND c. NAME = 'JPY'
		AND $X{IN,jor.id,order_id}
	) total_JPY,
	(
		SELECT
			sum(total_amount)
		FROM
			job_order_arap jor
		LEFT JOIN currency c ON c.id = jor.currency_id
		WHERE
			jor.order_type = 'charge'
		AND c. NAME = 'HKD'
		AND $X{IN,jor.id,order_id}
	) total_HKD,CONCAT(jos.vessel,'/',jos.voyage) vessel_voyage
FROM
	 job_order jo
LEFT JOIN job_order_arap jor ON jo.id = jor.order_id
LEFT JOIN party pr ON pr.id = jor.sp_id
LEFT JOIN job_order_shipment jos ON jos.order_id = jor.order_id
LEFT JOIN job_order_shipment_item josi ON josi.order_id = jo.id
LEFT JOIN unit u1 ON josi.unit_id = u1.id
LEFT JOIN unit u ON u.id = jor.unit_id
LEFT JOIN fin_item f ON f.id = jor.charge_id
LEFT JOIN currency c ON c.id = jor.currency_id
LEFT JOIN location lo1 ON lo1.id = jos.pol
LEFT JOIN location lo ON lo.id = jos.fnd
WHERE
	jor.order_type = 'charge'
AND  $X{IN,jor.id,order_id}]]>
	</queryString>
