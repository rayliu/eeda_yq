	<parameter name="order_id" class="java.lang.String"/>
	<parameter name="itemNo" class="java.lang.String"/>
	<parameter name="totalQuantity" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
	A.*, sum(ifnull(A.quantity, 0)) shelves_total_piece
FROM
	(
		SELECT
			gi.part_no,
			gi.quantity,
			pro.part_name,
			pro.item_no,
			pro.item_name,
			pro.amount,
			( $P{totalQuantity} * pro.amount) act_quantity,
		(
		SELECT
			GROUP_CONCAT(DISTINCT shelves SEPARATOR '    ')
		FROM
			gate_in
		WHERE
			out_flag = 'N'
		AND error_flag = 'N'
		and office_id = 1
		AND part_no = gi.part_no
		GROUP BY
			part_no
		) shelves
		FROM
			gate_in gi
		LEFT JOIN wmsproduct pro ON pro.part_no = gi.part_no
		WHERE
			gi.out_flag = 'N'
		AND gi.error_flag = 'N'
		AND gi.office_id = 1
		AND pro.item_no = $P{itemNo}
		GROUP BY
			gi.id
		UNION ALL
			SELECT
				pro.part_no,
				NULL quantity,
				pro.part_name,
				pro.item_no,
				pro.item_name,
				pro.amount,
				( $P{totalQuantity} * pro.amount) act_quantity,
				'' shelves
			FROM
				wmsproduct pro
			WHERE
				pro.amount > 0
			AND pro.office_id = 1
			AND pro.item_no = $P{itemNo}
	) A
GROUP BY
	A.part_no]]>
	</queryString>
