	<parameter name="order_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select A.*,count(A.shelves) this_box,sum(ifnull(A.quantity,0)) this_piece,
(select ifnull(count(gi.id),0) from gate_in gi
where gi.part_no = A.part_no and gi.out_flag = 'N' AND gi.error_flag = 'N') shelves_total_box,
(select ifnull(sum(gi.quantity),0) from gate_in gi
where gi.part_no = A.part_no and gi.out_flag = 'N' AND gi.error_flag = 'N') shelves_total_piece,
(select quantity*A.amount from gate_out_order where id = $P{order_id}) act_quantity
from (SELECT
	gi.part_no,
	gi.quantity,
	pro.part_name,
	pro.item_no,
	pro.item_name,
	pro.amount,
	(
		SELECT
			GROUP_CONCAT(DISTINCT shelves SEPARATOR '    ')
		FROM
			gate_in
		WHERE
			out_flag = 'N'
		AND error_flag = 'N'
		AND part_no = gi.part_no
		GROUP BY
			part_no
	) shelves
FROM
	gate_in gi
LEFT JOIN wmsproduct pro ON pro.part_no = gi.part_no
WHERE
	gi.out_order_id = $P{order_id}
AND gi.out_flag = 'N'
AND gi.error_flag = 'N'
union all
SELECT
	pro.part_no,
	null quantity,
	pro.part_name,
	pro.item_no,
	pro.item_name,
	pro.amount,
	null shelves
FROM
	wmsproduct pro
WHERE
 pro.amount>0
and pro.item_no = (select item_no from gate_out_order where id = $P{order_id})
) A group by A.part_no]]>
	</queryString>
