	<queryString>
		<![CDATA[SELECT aco.begin_time,aco.end_time,IFNULL(aco.own_company_name_eng,o.eng_office_name) eng_officeName,IFNULL(pwon.company_name,o.office_name) officeName,
IFNULL(pwon.homepage,o.homepage) homePage,IFNULL(aco.own_company_tel,o.office_tel) officeTel,
pwon.logo_path own_logo_path,o.logo_path logoPath,p.company_name,p.contact_person,p.phone,p.fax,
 u.c_name,IF(u.user_tel='' or u.user_tel=null,u.user_phone,u.user_tel) user_phone,u.email user_email,IFNULL(p.address,p.address_eng) company_address, jo.order_export_date,jo.order_no,jos.mbl_no MBL, jos.hbl_no HBL,
fi.name fee_name,joa.amount,joa.price,ut.name unit_name,
(SELECT IF (
  joa.order_type='charge',
	joa.total_amount,
	(0 - joa.total_amount)
) from job_order_arap joa WHERE joa.currency_id = 3 and joa.id = aci.ref_order_id ) cny,
(SELECT IF (
  joa.order_type='charge',
	joa.total_amount,
	(0 - joa.total_amount)
) from job_order_arap joa WHERE joa.currency_id = 6 and joa.id = aci.ref_order_id ) usd,

(SELECT IF (
  joa.order_type='charge',
	joa.total_amount,
	(0 - joa.total_amount)
) from job_order_arap joa WHERE joa.currency_id = 8 and joa.id = aci.ref_order_id ) jpy,
(SELECT IF (
  joa.order_type='charge',
	joa.total_amount,
	(0 - joa.total_amount)
) from job_order_arap joa WHERE joa.currency_id = 9 and joa.id = aci.ref_order_id ) hkd

from arap_charge_order aco
left join party p on p.id=aco.sp_id
LEFT JOIN party pwon on pwon.id = aco.own_company
left join user_login u on u.id = $P{login_user_id}
LEFT JOIN office o on o.id = aco.office_id
left join arap_charge_item aci on aci.charge_order_id = aco.id
left join job_order_arap joa on joa.id = aci.ref_order_id
LEFT JOIN fin_item fi on fi.id = joa.charge_id
LEFT JOIN unit ut on ut.id = joa.unit_id
left join job_order jo on jo.id = joa.order_id
left join job_order_shipment jos on jos.order_id = jo.id
where aco.id = $P{order_id}
GROUP BY aci.id]]>
	</queryString>
