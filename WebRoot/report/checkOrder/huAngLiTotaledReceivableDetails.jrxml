	<parameter name="order_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="login_user_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
aco.begin_time,aco.end_time,IFNULL(aco.own_company_name_eng,o.eng_office_name) eng_officeName,IFNULL(pwon.company_name,o.office_name) officeName,
IFNULL(pwon.homepage,o.homepage) homePage,IFNULL(aco.own_company_tel,o.office_tel) officeTel,pwon.logo_path own_logo_path,o.logo_path logoPath,p.company_name,
IFNULL(p.address,p.address_eng) company_address,p.contact_person,p.phone,p.fax, u.c_name,u.user_tel,u.email user_email,
jo.order_export_date,jo.order_no,jo.ref_no,p2.abbr cnzz,

CASE when l.name !='' then l.name when l1.name !='' then l1.name
else p1.abbr end fnd,
 replace(ifnull((SELECT GROUP_CONCAT(josi.container_type) from job_order_shipment_item josi  WHERE josi.order_id = jo.id),
  (SELECT GROUP_CONCAT(joli.truck_type) from job_order_land_item joli  WHERE joli.order_id = jo.id)),'\'','') truck_type,

(SELECT j.volume from job_order j where j.id = joa.order_id) CBM,

(SELECT j.pieces from job_order j where j.id = joa.order_id) CTNS,

(SELECT j.gross_weight from job_order j where j.id = joa.order_id) KGS,


ifnull(
(SELECT GROUP_CONCAT(josi.container_no) from job_order_shipment_item josi
where josi.order_id = joa.order_id),
(SELECT GROUP_CONCAT(j.land_container_no) from job_order_land_item j WHERE j.order_id = joa.order_id))
 container_no,
jos.mbl_no MBL, jos.hbl_no HBL,






(
		SELECT
			sum(joa.total_amount)-
                            IFNULL(( SELECT
			sum(joa.total_amount)
			FROM
			job_order_arap joa
		LEFT JOIN arap_charge_item aci ON joa.id = aci.ref_order_id
		LEFT JOIN arap_charge_order aco ON aco.id = aci.charge_order_id
		WHERE
			aco.id = $P{order_id}
		AND joa.currency_id = 6 AND joa.order_id = jo.id and joa.order_type = 'cost'),0)

		FROM
			job_order_arap joa
		LEFT JOIN arap_charge_item aci ON joa.id = aci.ref_order_id
		LEFT JOIN arap_charge_order aco ON aco.id = aci.charge_order_id
		WHERE
			aco.id = $P{order_id}
		AND joa.currency_id = 6 AND joa.order_id = jo.id and joa.order_type = 'charge'

	) usd,

(
		SELECT
			sum(joa.total_amount)-
                            IFNULL(( SELECT
			sum(joa.total_amount)
			FROM
			job_order_arap joa
		LEFT JOIN arap_charge_item aci ON joa.id = aci.ref_order_id
		LEFT JOIN arap_charge_order aco ON aco.id = aci.charge_order_id
		WHERE
			aco.id = $P{order_id}
		AND joa.currency_id = 3 AND joa.order_id = jo.id and joa.order_type = 'cost'),0)

		FROM
			job_order_arap joa
		LEFT JOIN arap_charge_item aci ON joa.id = aci.ref_order_id
		LEFT JOIN arap_charge_order aco ON aco.id = aci.charge_order_id
		WHERE
			aco.id = $P{order_id}
		AND joa.currency_id = 3 AND joa.order_id = jo.id and joa.order_type = 'charge' and charge_id != 68

	) cny,

(
		SELECT
			sum(joa.exchange_total_amount)-
                            IFNULL(( SELECT
			sum(joa.exchange_total_amount)
			FROM
			job_order_arap joa
		LEFT JOIN arap_charge_item aci ON joa.id = aci.ref_order_id
		LEFT JOIN arap_charge_order aco ON aco.id = aci.charge_order_id
		WHERE
			aco.id = $P{order_id}
		and charge_id = 68  AND joa.currency_id = 3 AND joa.order_id = jo.id and joa.order_type = 'cost'),0)

		FROM
			job_order_arap joa
		LEFT JOIN arap_charge_item aci ON joa.id = aci.ref_order_id
		LEFT JOIN arap_charge_order aco ON aco.id = aci.charge_order_id
		WHERE
			aco.id = $P{order_id}
		and charge_id = 68 AND joa.currency_id = 3 AND joa.order_id = jo.id and joa.order_type = 'charge'

	) ins

from job_order_arap joa
left join job_order jo on jo.id = joa.order_id
left join arap_charge_item aci on aci.ref_order_id = joa.id
left join arap_charge_order aco on aco.id = aci.charge_order_id
LEFT JOIN office o on o.id = aco.office_id
left join job_order_shipment jos on jos.order_id = jo.id
left join job_order_shipment_item josi on josi.order_id = jo.id
left join job_order_air_item joai on joai.order_id = jo.id
left join job_order_land_item joli on joli.order_id = jo.id
left join location l on l.id = jos.fnd
left join location l1 on l1.id = joai.destination
left join party p on p.id=aco.sp_id
LEFT JOIN party pwon on pwon.id = aco.own_company
left join party p1 on p1.id=joli.consignee
LEFT JOIN party p2 on p2.id = jos.MBLconsignee
left join user_login u on u.id = $P{login_user_id}
left join currency_rate cr on cr.currency_id = joa.currency_id
where aco.id = $P{order_id}
GROUP BY jo.id ORDER BY jo.order_export_date,jo.order_no]]>
	</queryString>
