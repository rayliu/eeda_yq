<script type="text/templete" id="form_select_field">
    <div class="mui-input-row">
        <label>{{display_name}}</label>
        <select name="{{name}}" class="" style="width:65%;">
            {{each item_list}}
                <option value="{{$value.VALUE}}" {{if value==$value.VALUE}} selected {{/if}} >{{$value.VALUE}}</option>
            {{/each}}
        </select>
    </div>
</script> 